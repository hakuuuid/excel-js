{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "object-curly-spacing": "off",
    "require-jsdoc": "off",
    "no-tabs": "off",
    "operator-linebreak": "off",
    "no-mixed-spaces-and-tabs": "off",
    "linebreak-style": "off"
  },
  "extends": [ "eslint:recommended", "google"]
}
